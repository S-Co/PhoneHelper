<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@+id/backgroud">
        <shape>
            <corners android:radius="10dp" />

            <solid android:color="@color/hardware_battery_bg" />
        </shape>
    </item>
    <item android:id="@+id/progress">
        <clip>
            <shape>
                <corners android:radius="10dp" />

                <solid android:color="@color/hardware_battery_progress" />
            </shape>
        </clip>
    </item>

</layer-list>